# Discord Bot Configuration
# Get these values from https://discord.com/developers/applications
DISCORD_TOKEN=your_discord_bot_token_here
CLIENT_ID=your_application_client_id_here
GUILD_ID=your_development_server_id_here

MONITOR_CHANNEL_ID=your_discord_channel_id

# 1MIN AI API Keys
# =================
AI_API_KEY=your_ai_api_key_here

# OSINT API Keys
# =================

# Whoxy API - Domain WHOIS data
# Get your API key from: https://www.whoxy.com/
WHOXY_API_KEY=your_whoxy_api_key_here

# DNSDumpster API - DNS reconnaissance
# Get your API key from: https://dnsdumpster.com/
DNSDUMPSTER_TOKEN=your_dnsdumpster_token_here

# Host.io API - Domain hosting intelligence
# Get your API key from: https://host.io/
HOSTIO_API_KEY=your_hostio_api_key_here

# Pappers API - French business registry
# Get your API key from: https://www.pappers.fr/
PAPPERS_API_KEY=your_pappers_api_key_here

# AVIATIONSTACK API - Flight tracker & airport
# Get your API key from: https://aviationstack.com/
AVIATIONSTACK_API_KEY=your_aviationstack_api_key_here

# AIRPORTDB -  Airports with their runways
# Get your API key from: https://airportdb.io/
AIRPORTDB_API_KEY=your_airportdb_api_key_here

# Nike TOKEN
NIKE_TOKEN=your_nike_token

# AWS Configuration for Rekognition
# ==================================
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-1

# External Tool Paths (Optional)
# ===============================
# If tools are not in your system PATH, specify full paths here

# ExifTool - Image metadata extraction
# Download from: https://exiftool.org/
EXIFTOOL_PATH=exiftool

# Sherlock - Username investigation
# Install from: https://github.com/sherlock-project/sherlock
SHERLOCK_PATH=sherlock

# Maigret - Enhanced username searches
# Install from: https://github.com/soxoj/maigret
MAIGRET_PATH=maigret

# Nuclei - Vulnerability scanner
# Install from: https://github.com/projectdiscovery/nuclei
NUCLEI_PATH=nuclei
NUCLEI_TEMPLATE_PATH=/username/nuclei-templates/http/osint/user-enumeration
# Additional Configuration
# ========================

# Bot Settings
BOT_PREFIX=!
DEFAULT_TIMEOUT=300
MAX_FILE_SIZE=50MB

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=discord-osint.log

# Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=10

# Security Settings
ALLOWED_GUILDS=
ADMIN_USERS=
